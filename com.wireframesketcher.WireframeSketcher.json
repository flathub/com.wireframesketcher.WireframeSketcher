{
  "id": "com.wireframesketcher.WireframeSketcher",
  "runtime": "org.gnome.Platform",
  "runtime-version": "47",
  "sdk": "org.gnome.Sdk",
  "command": "WireframeSketcher",
  "separate-locales": false,
  "tags": [
    "proprietary"
  ],
  "rename-appdata-file": "wireframesketcherstudio.appdata.xml",
  "rename-desktop-file": "wireframesketcherstudio.desktop",
  "rename-icon": "WireframeSketcher",
  "finish-args": [
    "--require-version=1.7.1",
    "--filesystem=host",
    "--filesystem=/tmp:rw",
    "--persist=.eclipse",
    "--share=network",
    "--share=ipc",
    "--socket=x11",
    "--allow=devel",
    "--socket=session-bus",
    "--device=dri"
  ],
  "modules": [
    {
      "name": "sketcher",
      "buildsystem": "simple",
      "build-options": {
        "no-debuginfo": true
      },
      "build-commands": [
      	". ${FLATPAK_BUILDER_BUILDDIR}/build.sh"
      ],
      "sources": [
        {
          "type": "file",
          "path": "apply_extra.sh",
          "dest-filename": "apply_extra"
        },
        {
          "type": "file",
          "path": "build.sh"
        },
        {
          "type": "extra-data",
          "filename": "sketcher.deb",
          "url": "http://cdn.wireframesketcher.com/studio/dist/WireframeSketcher-7.2.1_amd64.deb",
